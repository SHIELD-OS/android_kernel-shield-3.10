#include "tegra210-jetson-cv-base-p2597-2180-a00.dts"

/ {
	model = "foster_e";
	compatible = "nvidia,foster-e", "nvidia,tegra210";
	nvidia,dtsfilename = __FILE__;
	nvidia,fastboot-usb-pid = <0xb421>;

	chosen {
		fastboot-instructions = "Press <PWR> to select, <VOL D/REC> for selection move\n";
		board-has-eeprom;
	};

	firmware {
		android {
			compatible = "android,firmware";
			hardware = "foster_e";
		};
	};

	rollback-protection {
		status = "okay";
	};
	soctherm@0x700E2000 {
		throttle@oc3 {
			status = "disabled";
		};
	};
};